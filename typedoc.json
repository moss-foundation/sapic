{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Sapic",
  "out": "./autodocs",
  "entryPoints": [
    "crates/base",
    "crates/window",
    "crates/moss-activity-broadcaster",
    "crates/moss-project",
    "crates/moss-environment",
    "crates/moss-workspace",
    "crates/moss-bindingutils",
    "crates/ipc",
    "crates/moss-git",
    "crates/moss-language",
    "crates/moss-user",

    "plugins/shared_storage",
    "plugins/settings-storage"
  ],
  "entryPointStrategy": "packages",
  "packageOptions": {
    "includeVersion": false
  },
  "categorizeByGroup": false,
  "hideGenerator": true,
  "sort": ["kind", "alphabetical"],
  "excludeExternals": true,
  "useTsLinkResolution": true,
  "alwaysCreateEntryPointModule": true,
  "plugin": [
    "./misc/typedoc/typedoc-rename-plugin.js",
    "./misc/typedoc/typedoc-git-branch-plugin.js",
    "./misc/typedoc/typedoc-categorize-plugin.js"
  ]
}
